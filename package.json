{
  "name": "leoforge-neo",
  "version": "1.0.0",
  "description": "Modern Electron app combining LLM chat with Leo smart contract development environment",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "electron dist/main.js",
    "dev": "concurrently \"webpack --mode development --watch\" \"wait-on dist/main.js && electron dist/main.js\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "leo",
    "blockchain"
  ],
  "author": "LeoForge Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "css-loader": "^6.8.0",
    "electron": "^37.3.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "monaco-editor": "^0.52.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0"
  }
}
